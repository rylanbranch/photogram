<div class="posts-wrapper row">
  <div class="post">
    <div class="post-head">
      <div class="name">
        <p>Rylan Branch</p>
      </div>
    </div>
    <div class="image center-block">
      <%= image_tag(@post.image.url(:medium), class:'image-responsive') %>
    </div>
    <p class="caption">
      <%= @post.caption %>
    </p>
  </div>
  <p class="text-center edit-links">
    <%= link_to "Edit", edit_post_path %>
    <%= link_to "Back", posts_path %>
  </p>
</div>