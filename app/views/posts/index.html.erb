<div class="posts-wrapper row">
  <% @post.each do |post| %>
    <div class="post">
      <div class="post-head">
        <div class="name">
          <p>Rylan Branch</p>
        </div>
      </div>
      <div class="image center-block">
        <%= link_to image_tag(post.image.url(:medium), class:'image-responsive'), post_path(post) %>
      </div>
      <p class="caption">
        <%= post.caption %>
      </p>
    </div>
  <% end %>
</div>